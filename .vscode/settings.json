{
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [
                "/k",
                "doskey ojtest=oj t -c \"main.exe\" -d ./tests/"
            ],
            "env": {
                "PATH": "${workspaceRoot}\\bin;${env:PATH}",
                "VSCODE_WORKSPACE_ROOT": "${workspaceRoot}"
            },
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        }
    },
    "C_Cpp.default.compilerPath": "",
    "C_Cpp.default.cppStandard": "c++20",
    "files.associations": {
        "iostream": "cpp",
        "valarray": "cpp",
        "concepts": "cpp"
    }
}